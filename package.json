{
  "name": "react-redux-ut-codeceptjs",
  "version": "1.0.0",
  "description": "React + Redux + Webpack + Unit Test + CodeceptJS + Lint",
  "main": "index.js",
  "scripts": {
    "test": "mocha './tests/**/*.spec.js' --compilers js:babel-core/register",
    "mocha": "mocha",
    "webpack": "webpack",
    "webpack-dev-server": "webpack-dev-server",
    "start": "webpack-dev-server --hot --inline --config webpack.config.dev.js",
    "build:production": "webpack --config webpack.config.prod.js",
    "eslint": "eslint",
    "lint": "eslint -- ./",
    "codeceptjs": "codeceptjs",
    "e2e-test": "codeceptjs run --steps",
    "test-all": "npm run test && npm run lint && npm run e2e-test"
  },
  "author": "Gideon Rosales",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "codeceptjs": "^0.3.5",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "expect": "^1.20.1",
    "expect-jsx": "^2.5.1",
    "file-loader": "^0.8.5",
    "mocha": "^2.5.3",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-slider-monitor": "^1.0.5",
    "webdriverio": "^4.0.9",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "lodash": "^4.13.1",
    "node-uuid": "^1.4.7",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "superagent": "^2.0.0",
    "superagent-mocker": "^0.4.0"
  }
}
